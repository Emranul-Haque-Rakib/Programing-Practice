list = [1,2,3,4,5,6,7,8,9,13,24,56,65,21,32,34,43,56,64,77,87,78,90,37]
print('Please input the number')
number=int(input())
length=len(list)
i=0;


for i in range(i,length):
    if(i==number):
        print('FOund the number ',i)
    else:
        print('Number not Found')
        
        break
        
